<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.web.*?>

<BorderPane fx:id="root" maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0"
	prefWidth="800.0" styleClass="panel,panel-primary" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.swak.hello.MainPage">
	<top>
		<HBox fx:id="topPane" prefHeight="32.0" onDragDetected="#startMoveWindow"
			onMouseClicked="#titleDoubleClick" onMouseDragged="#moveWindow"
			onMouseReleased="#endMoveWindow" styleClass="panel-heading">
			<children>
				<ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true"
					preserveRatio="true">
					<image>
						<Image url="/images/logo.png" />
					</image>
					<HBox.margin>
						<Insets />
					</HBox.margin>
				</ImageView>
				<Pane prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
					<children>
						<Label minHeight="-Infinity" prefHeight="32.0" textFill="#fcf8f8"
							styleClass="h4">
							<font>
								<Font size="16.0" />
							</font>
							<padding>
								<Insets bottom="5.0" left="10.0" right="5.0" top="5.0" />
							</padding>
						</Label>
					</children>
				</Pane>
				<ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true"
					preserveRatio="true">
					<image>
						<Image url="/images/安全.png" />
					</image>
					<HBox.margin>
						<Insets left="5.0" right="5.0" />
					</HBox.margin>
				</ImageView>
				<ImageView onMouseClicked="#hide" fitHeight="32.0"
					fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="/images/托盘.png" />
					</image>
					<HBox.margin>
						<Insets left="5.0" right="5.0" />
					</HBox.margin>
				</ImageView>
				<ImageView onMouseClicked="#maximize" fitHeight="32.0"
					fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="/images/最大化.png" />
					</image>
					<HBox.margin>
						<Insets left="5.0" right="5.0" />
					</HBox.margin>
				</ImageView>
				<ImageView onMouseClicked="#close" fitHeight="32.0"
					fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="/images/关闭.png" />
					</image>
					<HBox.margin>
						<Insets left="5.0" />
					</HBox.margin>
				</ImageView>
			</children>
		</HBox>
	</top>
	<center>
	    <WebView fx:id="webView"/>
	</center>
</BorderPane>