<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>com.swak</groupId>
		<artifactId>swak</artifactId>
		<version>0.1.3.2_final</version>
	</parent>
	<artifactId>swak-starter</artifactId>
	<dependencies>
		<dependency>
			<groupId>com.swak</groupId>
			<artifactId>swak-core</artifactId>
		</dependency>

		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter</artifactId>
			<version>${spring-boot.version}</version>
			<exclusions>
				<exclusion>
					<groupId>org.springframework.boot</groupId>
					<artifactId>spring-boot-starter-logging</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.yaml</groupId>
					<artifactId>snakeyaml</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>com.swak</groupId>
			<artifactId>swak-flux</artifactId>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>com.swak</groupId>
			<artifactId>swak-vertx</artifactId>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>com.swak</groupId>
			<artifactId>swak-jdbc</artifactId>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>com.swak</groupId>
			<artifactId>swak-redis</artifactId>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>com.swak</groupId>
			<artifactId>swak-http</artifactId>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>com.swak</groupId>
			<artifactId>swak-storage</artifactId>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>com.swak</groupId>
			<artifactId>swak-motan</artifactId>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>com.swak</groupId>
			<artifactId>swak-mq</artifactId>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>com.swak</groupId>
			<artifactId>swak-rocketmq</artifactId>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>com.swak</groupId>
			<artifactId>swak-quartz</artifactId>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>org.freemarker</groupId>
			<artifactId>freemarker</artifactId>
			<version>${freemarker.version}</version>
			<scope>provided</scope>
		</dependency>
